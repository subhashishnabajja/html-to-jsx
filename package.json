{
	"name": "html-to-jsx",
	"displayName": "html-to-jsx",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascriptreact",
		"onLanguage:typescriptreact",
		"onLanguage:javascript",
		"onLanguage:html"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "html-to-jsx.convertToJsx",
				"title": "Convert Html to Jsx"
			},
			{
				"command": "html-to-jsx.convertToHtml",
				"title": "Convert Jsx to Html"
			},
			{
				"command": "html-to-jsx.pasteAsJsx",
				"title": "Paste as jsx"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorHasSelection",
					"command": "html-to-jsx.convertToJsx",
					"group": "modification"
				},
				{
					"when": "editorHasSelection",
					"command": "html-to-jsx.convertToHtml",
					"group": "modification"
				},
				{
					"when": "editorIsOpen",
					"command": "html-to-jsx.pasteAsJsx",
					"group": "modification"
				}
			]
		},
		"keybindings": [
			{
				"command": "html-to-jsx.pasteAsJsx",
				"key": "ctrl+alt+v"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/string.prototype.matchall": "^4.0.0",
		"@types/vscode": "^1.59.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"string.prototype.matchall": "^4.0.5"
	}
}